#registration-progress
  .circle-progress.viewed{ data: { target: '#registration-carousel', slide: { to: '0' } } }
    %h3 1
  .track-progress
  .circle-progress{ data: { target: '#registration-carousel', slide: { to: '1' } } }
    %h3 2
  .track-progress
  .circle-progress{ data: { target: '#registration-carousel', slide: { to: '2' } } }
    %h3 3
.carousel.slide#registration-carousel{ data: { ride: 'carousel', interval: 'false' } }
  .carousel-inner{ role: 'listbox' }
    .item
      = render partial: 'static/beta/privacy', locals: {privacy_page_content: @pc1}
    .item
      = render partial: 'static/beta/consent', locals: {consent_page_content: @pc2}
    .item.active
      = render partial: 'surveys/demographics'


/ = render partial: 'layouts/beta/registration/privacy_modal'
