- @active_top_nav_link = :participate

.row.reset-layout{ style: 'background-color: rgb(207, 226, 248)'}
  - question = @survey.questions.where(slug: 'sex').first
  - answer = @answer_session.answers.where(question_id: question.id).first
  - table_data = Report.frequency_data('sex', 1..2)
  .col-md-12.centered-text.lighterer-blue-bcg
    %h3.f500.text-dark7= question.text
    %p.lead.text-dark6= "Your answer: #{answer.show_value}"
    .row.reset-layout
      .col-xs-3.col-xs-offset-3
        = image_tag 'myapnea/surveys/about-me/male_color.png', class: 'img-responsive'
        %p.lead= "#{table_data[1][:text]}: #{table_data[1][:freq].round(1)}%"
      .col-xs-3
        = image_tag 'myapnea/surveys/about-me/female_color.png', class: 'img-responsive'
        %p.lead= "#{table_data[2][:text]}: #{table_data[2][:freq].round(1)}%"


