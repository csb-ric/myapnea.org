- @title = "Edit Team Member - #{@admin_team_member.name_was}"

- @theme = "blue-sky"
- content_for :header, @admin_team_member.name_was
= render "admin/tabs"
- content_for :breadcrumbs do
  %ol
    %li= link_to "team members", admin_team_members_path
    %li= link_to @admin_team_member.name_was.downcase, @admin_team_member
    %li.breadcrumb-muted edit
- content_for :sidebar, render("admin/sidebar")

.dashboard-container
  = render "form", admin_team_member: @admin_team_member
