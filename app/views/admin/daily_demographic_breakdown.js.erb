<% if @date1.present? and @date2.present? and @date1!=@date2 %>
  $("#summary-breakdown").html("There were <%= @users_by_date.count %> new users between <%= @date1.inspect %> and <%= @date2.inspect %>. Of these users, <%= @users_by_date.select{ |u| u.completed_demographic_survey? }.count %> completed the About Me survey.");
<% elsif @date1.present? %>
  $("#summary-breakdown").html("There were <%= @users_by_date.count %> new users on <%= @date1.inspect %>. Of these users, <%= @users_by_date.select{ |u| u.completed_demographic_survey? }.count %> completed the About Me survey.");
<% elsif @date2.present? %>
  $("#summary-breakdown").html("There were <%= @users_by_date.count %> new users on <%= @date2.inspect %>. Of these users, <%= @users_by_date.select{ |u| u.completed_demographic_survey? }.count %> completed the About Me survey.");
<% end %>
$("#summary-breakdown").append("<%=j render 'user_type_table', users: @users_by_date %>")
$("#sex-breakdown").html("<%=j render 'demographic_chart', demographic_data: Report.frequency_data('sex', 1..2, @users_by_date.pluck(:id)) %>")
$("#age-breakdown").html("<%=j render 'demographic_chart', demographic_data: @ages %>")
$("#racial-breakdown").html("<%=j render 'demographic_chart', demographic_data: Report.frequency_data('race', 1..5, @users_by_date.pluck(:id)) %>")
$("#ethnicity-breakdown").html("<%=j render 'demographic_chart', demographic_data: Report.frequency_data('ethnicity', 1..2, @users_by_date.pluck(:id)) %>")
$("#location-breakdown").html("<%=j render 'location_list', users: @users_by_date, selector: 'state' %>");
